let raceNumber = Math.floor(Math.random() * 1000);
const earlyRegistered = false
const runnerAge = 18

if (runnerAge>18 && earlyRegistered) {
console.log ('ready to go at 9:30am');
console.log("Your number is" + " " +raceNumber);}
  else if (runnerAge>18 && !earlyRegistered) {
  console.log('Late adults run at 11');
  console.log("Your number is" + " " +raceNumber);}
  else if (runnerAge < 18) {
  console.log('Youth run at 12:30');
  console.log("Your number is" + " " +raceNumber);}

else {
  console.log('See registration desk')
}
